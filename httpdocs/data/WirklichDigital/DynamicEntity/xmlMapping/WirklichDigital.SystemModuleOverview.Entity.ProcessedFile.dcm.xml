<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WirklichDigital\SystemModuleOverview\Entity\ProcessedFile" repository-class="WirklichDigital\DynamicEntityModule\Repository\DynamicEntityRepository">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="filename" type="string"/>
    <field name="filePath" type="string" nullable=""/>
    <field name="fileHash" type="string" nullable=""/>
    <field name="processedAt" type="datetime" nullable=""/>
    <field name="status" type="string" nullable=""/>
    <field name="errorMessage" type="text" nullable="1"/>
  </entity>
</doctrine-mapping>
