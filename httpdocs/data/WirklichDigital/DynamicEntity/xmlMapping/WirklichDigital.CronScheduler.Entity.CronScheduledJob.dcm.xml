<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WirklichDigital\CronScheduler\Entity\CronScheduledJob" repository-class="WirklichDigital\DynamicEntityModule\Repository\DynamicEntityRepository">
    <indexes>
      <index name="index_name" columns="name"/>
      <index name="index_priority" columns="priority"/>
      <index name="index_isRunning" columns="isRunning"/>
    </indexes>
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string"/>
    <field name="isEnabled" type="boolean" nullable="">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="cronString" type="string" length="50" nullable="">
      <options>
        <option name="default">* * * * *</option>
      </options>
    </field>
    <field name="isRunning" type="boolean" nullable="">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lastResult" type="text" nullable="">
      <options>
        <option name="default"></option>
      </options>
    </field>
    <field name="lastExecutionError" type="boolean" nullable="">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lastExecutionDuration" type="decimal" nullable="" precision="10" scale="4">
      <options>
        <option name="default">0.0000</option>
      </options>
    </field>
    <field name="lastInvocationTime" type="datetime" nullable="1"/>
    <field name="autoRestartAfter" type="integer" nullable="">
      <options>
        <option name="default">120</option>
      </options>
    </field>
    <field name="priority" type="integer" nullable="">
      <options>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
