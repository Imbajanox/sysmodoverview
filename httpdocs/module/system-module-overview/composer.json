{
    "name": "wirklich-digital/system-module-overview",
    "description": "SystemModuleOverview",
    "type": "library",
    "authors": [
        {
            "name": "Wirklich Digital",
            "email": "info@wirklich.digital"
        }
    ],
    "autoload": {
        "psr-4": {
            "WirklichDigital\\SystemModuleOverview\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WirklichDigital\\SystemModuleOverviewTest\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laminas": {
            "module": "WirklichDigital\\SystemModuleOverview"
        }
    },
    "require": {
        "laminas/laminas-modulemanager": "^2.12"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3",
        "phpunit/phpunit" : "^10.0"
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://h17.jarmedia.de/satis/packages"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
